	"Keccak compare": {
		"prefix": "kec",
		"body": [
			"if (keccak256(abi.encodePacked($1)) == keccak256(abi.encodePacked($2))) {$0}",
		],
		"description": "Keccak256 compare"
	},

	"New contract template": {
		"prefix": "newContract",
		"isFileTemplate": true,
		"body": [
			"// SPDX-License-Identifier: GPL-3.0", 
			"",
			"pragma solidity 0.8.17;",
			"",
			"/**",
			"* @title ${1:MyContract}",
			"*/",
			"contract $1 {",
			"    $0",
			"}"
		]
	}
}